{
	"name": "homepage",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"prebuild": "tsx scripts/generate-og-images.ts",
		"build": "vite build",
		"preview": "pnpm run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "pnpm run build && wrangler deploy",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"generate-og": "tsx scripts/generate-og-images.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@shikijs/colorized-brackets": "^3.17.0",
		"@shikijs/twoslash": "^3.17.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24.10.2",
		"@types/react": "^19.2.7",
		"gray-matter": "^4.0.3",
		"hono": "^4.11.4",
		"mdsvex": "^0.12.6",
		"playwright": "^1.57.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-preset-minify": "^7.0.1",
		"rehype-slug": "^6.0.0",
		"remark-breaks": "^4.0.0",
		"remark-lint": "^10.0.1",
		"svelte": "^5.45.2",
		"svelte-check": "^4.3.4",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.4",
		"vite-plugin-top-level-await": "^1.6.0",
		"vite-plugin-wasm": "^3.5.0",
		"wrangler": "^4.51.0"
	},
	"dependencies": {
		"@fontsource/google-sans-code": "^5.2.3",
		"@hono/valibot-validator": "^0.6.0",
		"@radix-ui/themes": "^3.2.1",
		"@resvg/resvg-wasm": "^2.6.2",
		"@shikijs/rehype": "^3.17.0",
		"katex": "^0.16.25",
		"rehype-katex": "^7.0.1",
		"rehype-raw": "^7.0.0",
		"rehype-stringify": "^10.0.1",
		"rehype-twoslash": "^1.1.1",
		"remark-math": "^6.0.0",
		"satori": "^0.18.3",
		"satori-html": "^0.3.2",
		"shiki": "^3.17.0",
		"valibot": "^1.2.0"
	}
}
